{
    "name": "wersas1/cfscrape",
    "type": "library",
    "description": "A PHP module to bypass Cloudflare's anti-bot page.",
    "keywords": [
        "Cloudflare",
        "ScoLib",
        "cfscrape"
    ],
    "homepage": "https://github.com/ScoLib/cfscrape",
    "license": "MIT",
    "authors": [
        {
            "name": "klgd",
            "email": "slice1213@gmail.com",
            "homepage": "https://github.com/klgd",
            "role": "Developer"
        },
        {
            "name": "wersas1",
            "email": "wersas1@gmail.com",
            "homepage": "https://github.com/wersas1",
            "role": "Contributor"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-v8js": "*",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=9.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Cfscrape\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
